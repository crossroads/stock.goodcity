{{#if item.setImages.length}}
  {{#slick-carousel class="ember-carousel" autoplay=true autoplaySpeed=2500 speed=500 swipeToSlide=true infinite=false  mobileFirst=true touchMove=true slidesToShow=1 slidesToScroll=1}}
    {{#each item.setImages as |image|}}
      <div data-src={{image}} class="imageZoom" >
        {{#if isMobileApp}}
          <img src={{image}} {{action "imageZoom" image}} class="profile slide-image"/>
        {{else}}
          <img src={{image}} class="profile"/>
        {{/if}}
      </div>
    {{/each}}
  {{/slick-carousel}}
{{/if}}
